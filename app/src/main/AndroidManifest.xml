<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iiitlucknowlibrary">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/book"
        android:label="@string/app_name"
        android:roundIcon="@drawable/book"
        android:supportsRtl="true"
        android:theme="@style/Theme.IIITLucknowLibrary">
        <activity
            android:name=".administration.ReturnBook"
            android:exported="true" />
        <activity
            android:name=".administration.UpdateBook"
            android:exported="true" />
        <activity
            android:name=".administration.RemoveABook"
            android:exported="true" />
        <activity
            android:name=".administration.IssueBook"
            android:exported="true" />
        <activity
            android:name=".administration.AddBook"
            android:exported="true" />
        <activity
            android:name=".HomeAdministration"
            android:exported="true" />
        <activity android:name=".Authentication.Registration" />
        <activity android:name=".Home" />
        <activity android:name=".Authentication.Login" />
        <activity
            android:name=".SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>